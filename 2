
#include <iostream>
using namespace std;
int main()
{
    int m, d, c, kol, ost;
    kol = 0;
    ost = 365;
    setlocale(LC_ALL, "Rus");
    cin >> m >>d;
    if (m == 1 && d > 0 && d < 32)
        cout << "до Нового года осталось " << 365 - d << " дней";
    else if (m == 2 && d > 0 && d < 29)
        cout << "до Нового года осталось " << 334 - d << " дней";
    else if (m == 3 && d > 0 && d < 32)
        cout << "до Нового года осталось " << 306 - d << " дней";
    else if (m == 4 && d > 0 && d < 31)
        cout << "до Нового года осталось " << 275 - d << " дней";
    else if (m == 5 && d > 0 && d < 32)
        cout << "до Нового года осталось " << 245 - d << " дней";
    else if (m == 6 && d > 0 && d < 31)
        cout << "до Нового года осталось " << 214 - d << " дней";
    else if (m == 7 && d > 0 && d < 32)
        cout << "до Нового года осталось " << 184 - d << " дней";
    else if (m == 8 && d > 0 && d < 32)
        cout << "до Нового года осталось " << 153 - d << " дней";
    else if (m == 9 && d > 0 && d < 31)
        cout << "до Нового года осталось " << 122 - d << " дней";
    else if (m == 10 && d > 0 && d < 32)
        cout << "до Нового года осталось " << 92 - d << " дней";
    else if (m == 11 && d > 0 && d < 31)
        cout << "до Нового года осталось " << 61 - d << " дней";
    else if (m == 12 && d > 0 && d < 32)
        cout << "до Нового года осталось " << 31 - d << " дней";
    else
        cout << "ошибка";
    
    return 0;
}
